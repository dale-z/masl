
class People
{
	state Rich 
	{
		if(money - 1000000 < 0.1) this->Poor;
	}

	state Poor 
	{
		if(neighborRicherCount(0, [class People]{class People()}) > 3) 
			money = money + 5000;
		if(money - 1000000 > 0.1) this->Rich;
	}

	double money;
		
	int neighborRicherCount(int a, [class People] b)
	{
		a = 3;
		class People p = b:.get(0);
		return a;
	}
	
	int age;
	char sex;
}

